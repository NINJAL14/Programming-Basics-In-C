int main() { 
// Declare an array of size 5 
int numbers[5]; 
// Taking input from the user printf("Enter 5 integers:\n"); 
for (int i = 0; i < 5; i++) { 
} 
scanf("%d", &numbers[i]); 
// Print the elements in the array 
printf("The entered integers are:\n"); for (int i = 0; i < 5; i++) { 
printf("%d\n", numbers[i]); 
} 
} 
return 0; 
