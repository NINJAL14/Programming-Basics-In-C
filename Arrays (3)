#include <stdio.h> 
int main() { 
// Declare a 2D array of size 3x3 
int matrix[3][3]; 
// Taking input from the user 
printf("Enter 9 integers to fill the 3x3 matrix:\n"); 
for (int i = 0; i < 3; i++) { 
for (int j = 0; j < 3; j++) { 
scanf("%d", &matrix[i][j]); 
} 
} 
} 
// Print the 2D array 
printf("The entered 3x3 matrix is:\n"); 
for (int i = 0; i < 3; i++) { 
for (int j = 0; j < 3; j++) { 
printf("%d\t\t", matrix[i][j]); 
} 
printf("\n"); 
} 
return 0;
